mixin footer(index)
  footer.footer
    .wrapper
      .footer__row

        .footer__col
          .footer__logo-description Франшиза коворкинг-пространства
        .footer__col
          .footer__logo
            img(src="images/logo-main.png" alt="logo" width="110" height="80").d-block
        .footer__col
          a(href="#" data-remodal-target="privacy").footer__privacy.font-m-reg.color-gold-1.d-block Политика конфиденциальности
